<script type="text/javascript" src="/FEPA/min/ECommerce_FEPA__step_1_short_1_171129094101411.js"></script>
    <script type="text/javascript">
        function abilitaForm(id)
        {
            $("#" + id + " input").not("input[type='hidden']").prop("disabled", false);
            $("#" + id + " select").prop("disabled", false);
            $("#" + id).find('.barrapulsanti > input[type="submit"]').show();
        }
        function disabilitaForm(id)
        {
            $("#" + id + " input").not("input[type='hidden']").prop("disabled", true);
            $("#" + id + " select").prop("disabled", true);
        }
        function resetAndHideForm(id)
        {
            $("#" + id).fadeOut();
            $("#" + id + " input[type='checkbox']").prop("checked", false);
            $("#" + id + " input[type='text'],input[type='email']").val("");
            $("#" + id + " select").val("");
        }
        function hideAndUnbind(selector)
        {
            $(selector).hide();
            $(selector).unbind("click");
        }
        function showEditPIvaLink()
        {
            $("a.editPiva").show();
            $("#dvSpaceBottom").hide();
            $('#formAcquisto .messaggioErrore').html("");
            $("a.editPiva").on("click", function ()
            {
                // scrollo verso la form
                $('html, body').animate({
                    scrollTop: $("#formControllaIVA").offset().top
                }, 500);
                // abilito formControllaIVA
                abilitaForm("formControllaIVA");
                // Reset And Hide 'formContratto' e 'formAcquisto'
                resetAndHideForm("formContratto");
                     resetAndHideForm("formAcquisto");
                    // nascondo il link 'Modifica' e unbind
                    hideAndUnbind("a.editPiva");
                $("#dvSpaceBottom").show();
                    return false;
                });
        }
        function showNextStep(d)
        {
            // mostro e scrollo verso la form
            $("#formAcquisto").show();
            $('html, body').animate({
                scrollTop: $("#formAcquisto").offset().top
            }, 500);
            // riempio i campi hidden della form
            $("#PIva").val(d.PIva);
            $("#CodProdotto").val(d.CodProdotto);
            $("#tipoUtente").val(d.tipoUtente);
            
            $("#Campagna").val(d.Campagna);
            $("#CodicePromo").val(d.CodicePromo);
            $("#rivend").val(d.pagabon);
            $("input[name='Piva']").val(d.PIva);

            
            // FORM Step 2
            $('#formAcquisto').validate({
                rules: { Cap: { digits: true } },
                messages: { Cap: "specificare Cap valido" },
                callbackHandler: function (data, form)
                {
                    $(form).find('.barrapulsanti > .loading').hide();
                    if (data.d.Status)
                    {
                        // rimuovo submit
                        $(form).find('.barrapulsanti > input[type="submit"]').hide();
                        // disabilito la form
                        disabilitaForm("formAcquisto");
                        $('#formAcquisto .messaggioErrore').html("");
                        // mostra terza parte della form
                        showNextStep2(data.d);
                    } else
                    {
                        $('#formAcquisto .messaggioErrore').html(data.d.Props.messaggio);
                        _gaq.push(['_trackEvent', 'Errore BBS', 'STEP2::' + data.d.Props.messaggio]);
                        $('#formAcquisto .messaggioErrore').css('opacity', '1');
                        $(form).find('.barrapulsanti > input[type="submit"]').show();
                    }
                }
            });
        }
        function showEditDataLink()
        {
            $("a.editStep2").show();
            $("a.editStep2").on("click", function ()
            {
                // scrollo verso la form e abilito i campi
                $('html, body').animate({
                    scrollTop: $("#formAcquisto").offset().top
                }, 500);
                // abilito formAcquisto
                abilitaForm("formAcquisto");
                // nascondo l'altra form
                $("#formContratto").fadeOut();
                // Reset And Hide 'formContratto''
                resetAndHideForm("formContratto");
                // nascondo il link 'Modifica' e unbind
                hideAndUnbind("a.editStep2");
                return false;
            });
        }
        function showNextStep2(d)
        {
            // Studio/Azienda
            /*if ($("#tipologia").is(":checked"))
            {
                $(".forStudio").hide();
                $(".forAzienda").show();
            }
            else
            {
                $(".forStudio").show();
                $(".forAzienda").hide();
            }*/
            if ($("#rivend").val() == "1") {
                $(".doppipulsanti").show();
                $(".singolopulsante").hide();
            } else {
                $(".doppipulsanti").hide();
                $(".singolopulsante").show();
            }

            // mostro il link per la modifica dei dati anagrafici
            showEditDataLink();
            // Contratto e Clausole Vessatorie
            $("#contratto").html(d.Props.CONTRATTO);
            $("#clVess").tooltip("option", "content", ("artt. " + d.Props.CL_VESS));
            // mostro e srollo verso la form
            $("#formContratto").show();
            $('html, body').animate({
                //scrollTop: $("#formCondizioni").offset().top
                scrollTop: $("#formContratto div:eq(0)").offset().top
            }, 500);
            // fancybox
            $("#formContratto a").not(".tt").not("#clVess").fancybox();

            // FORM Step 3 - Pay Pal
            $('#formContratto').validate({
                callbackHandler: function (data, form)
                {
                    if (data.d.Status)
                    {
                        if (data.d.Props.url)
                            $('#paymentBoxStep').GTMVariables().call().then(function ()
                            {
                                document.location = data.d.Props.url;
                            });
                    } else
                    {
                        $('#messaggioErrore').html(data.d.Props.messaggio);
                        _gaq.push(['_trackEvent', 'Errore BBS', 'STEP3::' + data.d.Props.messaggio]);
                        $('#messaggioErrore').css('opacity', '1');
                        $(form).find('.barrapulsanti > .loading').each(function ()
                        {
                            var barra = $(this).parent();
                            barra.children().not(this).show();
                            $(this).hide();
                        });
                    }
                }
            });
        }
        // INIT
        $(function ()
        {
            // hide elements
 
            $("#formAcquisto").hide();
            $("#formContratto").hide();
                         $("a.editPiva").hide();
            $("#dvSpaceBottom").show();
            $("a.editStep2").hide();
            // jquery ui tooltip
            $("#clVess").tooltip({
                items: 'span',
                content: 'clausole vessatorie' // replaced after ajax - row 160
            });
            $(".tt").tooltip({
                content: function ()
                {
                    return $(this).attr('title');
                }
            });
            // FORM Step 1
            $('#formControllaIVA').validate({
                callbackHandler: function (data, form)
                {
                    if (data.d.Status)
                    {
                        if (data.d.Props.url)
                        {
                            $('#formControllaIVA .messaggioErrore').css('opacity', '0');

                            var gtm = $('.gtm, .boxStep.on:last').GTMVariables();
                            gtm.numeroStep = 2;
                            gtm.nomeStep = "/FEPA/dati_anagrafici";
                            gtm.labelEvento = null;
                            gtm.call();
                            //console.log(gtm);

                            // rimuovo il loading
                            $(form).find('.barrapulsanti > .loading').hide();
                            // disabilito la form
                            disabilitaForm("formControllaIVA");
                            // mostro il link per la modifica
                            showEditPIvaLink();
                            // mostra seconda parte della form
                            showNextStep(data.d.Props);
                        } else
                        {
                            $('#formControllaIVA .messaggioErrore').html(data.d.Props.messaggio);
                            _gaq.push(['_trackEvent', 'Errore BBS', 'STEP1::' + data.d.Props.messaggio]);
                            $('#formControllaIVA .messaggioErrore').css('opacity', '1');
                            $(form).find('.barrapulsanti > .loading').each(function ()
                            {
                                var barra = $(this).parent();
                                barra.children().not(this).show();
                                $(this).hide();
                            });
                        }
                    } else
                    {
                        $('#formControllaIVA .messaggioErrore').html(data.d.Props.messaggio);
                        _gaq.push(['_trackEvent', 'Errore BBS', 'STEP1::' + data.d.Props.messaggio]);
                        $('#formControllaIVA .messaggioErrore').css('opacity', '1');
                        $(form).find('.barrapulsanti > .loading').each(function ()
                        {
                            var barra = $(this).parent();
                            barra.children().not(this).show();
                            $(this).hide();
                        });
                    }
                }
            });
        });
    </script>

