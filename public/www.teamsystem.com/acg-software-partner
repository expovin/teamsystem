                                $.fancybox({ content: '<div class="fancy_text">' + "Si è verificato un errore, controlla i dati "  + '</div>' });
                            }else{
                                $.fancybox.open('<div class="fancy_text">' + "Si è verificato un errore, controlla i dati "  + '</div>');
                            }
                        }catch(e){}
                    }
                }
            });
            $('a.privacy').fancybox({
                padding: 10,
                // margin: 20,
                //autoScale: true,
                transitionIn: 'elastic',
                transitionOut: 'elastic',
                easingIn: 'easeOutBack',
                easingOut: 'easeInBack',
                overlayColor: '#000',
                width: 620,
                height: 470,
                type: 'iframe'
            });
        });



    </script>
                                    </div>
                            </div>
                        </div>
                    </section>
